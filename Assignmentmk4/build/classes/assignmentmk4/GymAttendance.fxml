<AnchorPane id="AnchorPane" prefHeight="605.0" prefWidth="938.0" style="-fx-background-color: #2e8b57;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignmentmk4.GymAttendanceController">
   <children>
      <AnchorPane layoutX="26.0" layoutY="148.0" prefHeight="392.0" prefWidth="495.0" style="-fx-background-color: white;">
         <children>
            <LineChart fx:id="GymAttendance" layoutX="14.0" layoutY="29.0" prefHeight="334.0" prefWidth="446.0" title="Gym Attendance">
              <xAxis>
                <CategoryAxis fx:id="day" label="Day" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="sessions" label="Session" side="LEFT" />
              </yAxis>
            </LineChart>
         </children>
      </AnchorPane>
      <Label layoutX="253.0" layoutY="30.0" prefHeight="35.0" prefWidth="461.0" text="Gym  Attendance" textFill="#f5f5f5">
         <font>
            <Font name="Arial Black" size="48.0" />
         </font>
      </Label>
      <Button layoutX="26.0" layoutY="30.0" mnemonicParsing="false" onAction="#returnHome" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="72.0" fitWidth="86.0">
               <image>
                  <Image url="@../../AppData/Local/Temp/icons8_left_arrow_52px_3.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <AnchorPane layoutX="545.0" layoutY="148.0" prefHeight="392.0" prefWidth="364.0" style="-fx-background-color: white;">
         <children>
            <Button fx:id="saveAttendance" layoutX="136.0" layoutY="307.0" mnemonicParsing="false" onAction="#saveAttendance" style="-fx-background-color: transparent;" />
            <DatePicker fx:id="dateAttendance" layoutX="54.0" layoutY="196.0" prefHeight="46.0" prefWidth="239.0" promptText="Select Date" />
            <TextField fx:id="sessionsAttendance" layoutX="54.0" layoutY="87.0" prefHeight="46.0" prefWidth="239.0" promptText="Enter no. of sessions" />
            <ImageView fitHeight="87.0" fitWidth="95.0" layoutX="135.0" layoutY="289.0">
               <image>
                  <Image url="@../../AppData/Local/Temp/icons8_ok_60px.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>


